{
    "$schema": "https://schema.railpack.com",
    "buildAptPackages": ["git","curl","r-base"],
    "packages": {
      "python": "3.12.10"
    },
    "steps": {
      "install": {
        "commands": ["python -m venv /app/.venv", "pip install -r requirements.txt"]
      }
    },
    "deploy": { 
      "startCommand": "python main.py"
    }
  }